 <script>
 var nav = $("nav.menu");
//add indicators and hovers to submenu parents
nav.find("li").each(function() {
    if ($(this).find("ul").length > 0) {

        $("<span>").text("^").appendTo($(this).children(":first"));

        //show subnav on hover
        $(this).click(function() {
            $(this).find("ul").stop(true, true).slideToggle();
        });
    }
});    
 </script>  
  <script>
function showBlueMenuContent(str)
{
if (str=="")
  {
  document.getElementById("contentBlueValue").innerHTML="";
  return;
  }
 // alert(str);
if (window.XMLHttpRequest)
  {// code for IE7+, Firefox, Chrome, Opera, Safari
  xmlhttp=new XMLHttpRequest();
  }
else
  {// code for IE6, IE5
  xmlhttp=new ActiveXObject("Microsoft.XMLHTTP");
  }
xmlhttp.onreadystatechange=function()
  {
  if (xmlhttp.readyState==4 && xmlhttp.status==200)
    {
    document.getElementById("contentBlueValue").innerHTML=xmlhttp.responseText;
    }
  }
xmlhttp.open("GET","<?php echo get_template_directory_uri(); ?>/getContent.php?postTitle="+str,true);
xmlhttp.send();
