//validation on contact & request forms
function phonenumberOnblur(id,val)  
    {  
      var valId =id;
      var errorId ='error'+id;
      var phonenoVal =val;  
      var phoneno = /^[\+]?[(]?[0-9]{3}[)]?[-\s\.]?[0-9]{3}[-\s\.]?[0-9]{4,6}$/im;  
      
  if(phonenoVal.length>0){   
              if(phoneno.test(phonenoVal))  
            {
         document.getElementById(errorId).innerHTML='';
            return true;
            }  
          else  
            {  

            document.getElementById(errorId).innerHTML='Phone number is invalid';
           
             return false;
            }  
           }else{
        document.getElementById(errorId).innerHTML='';
           }
    }
